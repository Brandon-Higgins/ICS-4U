public class BirthdayGame {
  public static void main (String[] args) {
      System.out.println("Please enter your birth month...");
      
      Scanner monthScanner = new Scanner(System.in);
      int month = monthScanner.nextInt();
      
      int int1 = month*5;
      int int2 = int1+6;
      int in3 = int*4;
      int int4 = int3+9;
      int int5 = int4*5;
      
      System.out.println("Please enter the day of your birth...");
      Scanner dayScanner = new Scanner(System.in);
      int day = dayScanner.nextInt();
      
      int int6 = int5+day;
      int birthday = int6-165;
      int birthday2 = birthday/100;
      System.out.println(birthday2);
      
  }
}
